{
  "name": "antd-components",
  "version": "1.0.0",
  "description": "基于antd开发的组件",
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,dist,es,lib,test}/**/*.{js,jsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "betterScripts": {
    "build": {
      "command": "webpack"
    },
    "start": {
      "command": "webpack-dev-server"
    },
    "test": {
      "command": "jest --config jest.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/longshenpan/antd-components.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/longshenpan/antd-components/issues"
  },
  "homepage": "https://github.com/longshenpan/antd-components#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.1",
    "eslint": "^5.3.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.1",
    "react": "^16.4.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "antd": "^3.8.1",
    "moment": "^2.22.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1"
  }
}
